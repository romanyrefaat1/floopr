{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ]
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install --legacy-peer-deps",
  "crons": [
    {
      "path": "/api/ml/crons/prioritize",
      "schedule": "37 22 * * *"
    }
  ],
  "routes": [
    {
      "src": "/api/webhooks/dodo",
      "methods": ["POST"],
      "continue": true
    }
  ]
}
